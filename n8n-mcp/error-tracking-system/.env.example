# Application Configuration
NODE_ENV=development
API_PORT=3007
CORS_ORIGIN=http://localhost:3000,http://localhost:5173

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=error_tracking
DB_USERNAME=postgres
DB_PASSWORD=password
DB_SSL=false
DB_MAX_CONNECTIONS=20

# Error Tracking Configuration
ENABLE_REALTIME_ALERTS=true
ENABLE_ERROR_GROUPING=true
ENABLE_BREADCRUMBS=true
ERROR_RETENTION_DAYS=30
ENABLE_SOURCE_MAPS=false
ERROR_SAMPLING_RATE=1.0

# Alert Configuration
ENABLE_REALTIME_ALERTS=true
ALERT_RETENTION_DAYS=90
ENABLE_ALERT_METRICS=true
ESCALATION_DELAY=1800000
MAX_ESCALATION_LEVEL=5
ENABLE_AUTO_ESCALATION=true
ENABLE_SCHEDULED_CHECK=true

# Notification Configuration
ENABLE_NOTIFICATION_RETRIES=true
NOTIFICATION_RETENTION_DAYS=30
ENABLE_NOTIFICATION_METRICS=true

# Notification Channels
ENABLE_EMAIL_NOTIFICATIONS=true
ENABLE_SLACK_NOTIFICATIONS=true
ENABLE_DISCORD_NOTIFICATIONS=true
ENABLE_SMS_NOTIFICATIONS=true
ENABLE_WEBHOOK_NOTIFICATIONS=true
ENABLE_PUSH_NOTIFICATIONS=true

# Email Configuration (SMTP)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password
SMTP_FROM=your-email@gmail.com

# Slack Configuration
SLACK_TOKEN=xoxb-your-slack-bot-token
SLACK_CHANNEL=#alerts

# Discord Configuration
DISCORD_TOKEN=your-discord-bot-token
DISCORD_CHANNEL=channel-id

# Twilio SMS Configuration
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_FROM_NUMBER=+1234567890

# API Configuration
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100
ENABLE_WEBSOCKET=true
ENABLE_API_METRICS=true

# External Integrations
SENTRY_DSN=your-sentry-dsn
DATADOG_API_KEY=your-datadog-api-key
DATADOG_APP_KEY=your-datadog-app-key
DATADOG_SITE=datadoghq.com
NEWRELIC_LICENSE_KEY=your-newrelic-license-key
NEWRELIC_APP_NAME=n8n-error-tracking
ELASTICSEARCH_HOST=http://localhost:9200
ELASTICSEARCH_INDEX=error-tracking
ELASTICSEARCH_USERNAME=elastic
ELASTICSEARCH_PASSWORD=password

# Logging Configuration
LOG_LEVEL=info