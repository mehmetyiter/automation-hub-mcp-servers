# Environment Configuration
NODE_ENV=development
LOG_LEVEL=info

# Server Configuration
PORT=3010
HOST=0.0.0.0
ENABLE_CORS=true
CORS_ORIGIN=*
ENABLE_RATE_LIMIT=true
MAX_REQUESTS_PER_MINUTE=200

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=predictive_analytics
DB_USERNAME=postgres
DB_PASSWORD=password
DB_SSL=false
DB_MAX_CONNECTIONS=20

# Predictive Analytics Configuration
ENABLE_AUTO_RETRAIN=true
ENABLE_ANOMALY_DETECTION=true
ENABLE_OPTIMIZATION_RECOMMENDATIONS=true
MAX_MODELS=50
PREDICTION_CACHE_TTL=3600000
MODEL_RETRAIN_INTERVAL=86400000
MIN_DATA_POINTS=100
CONFIDENCE_THRESHOLD=0.7

# Optimization Configuration
ENABLE_WORKFLOW_OPTIMIZATION=true
ENABLE_RESOURCE_OPTIMIZATION=true
ENABLE_PERFORMANCE_OPTIMIZATION=true
ENABLE_COST_OPTIMIZATION=true
OPTIMIZATION_INTERVAL=14400000
MIN_DATA_POINTS_OPTIMIZATION=100

# Machine Learning Configuration
ENABLE_TENSORFLOW=true
ENABLE_GPU=false
MODEL_SAVE_DIR=./models
MAX_TRAINING_EPOCHS=100
DEFAULT_BATCH_SIZE=32
VALIDATION_SPLIT=0.2
EARLY_STOPPING_PATIENCE=10

# Cache Configuration
ENABLE_REDIS=false
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=
CACHE_DEFAULT_TTL=3600
REDIS_MAX_MEMORY_POLICY=allkeys-lru

# Monitoring Configuration
ENABLE_PROMETHEUS=true
PROMETHEUS_PORT=9090
ENABLE_HEALTH_CHECKS=true
HEALTH_CHECK_INTERVAL=30000
ENABLE_PERFORMANCE_TRACKING=true

# Security Configuration
ENABLE_AUTHENTICATION=false
JWT_SECRET=your-super-secret-jwt-key
JWT_EXPIRES_IN=24h
API_KEY_HEADER=X-API-Key
ENABLE_API_KEYS=false
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=200

# Feature Flags
ENABLE_EXPERIMENTAL_MODELS=false
ENABLE_AUTO_HYPERPARAMETER_TUNING=false
ENABLE_DISTRIBUTED_TRAINING=false
ENABLE_MODEL_VERSIONING=true
ENABLE_ADVANCED_ANOMALY_DETECTION=false
ENABLE_REALTIME_PREDICTIONS=true
ENABLE_MODEL_EXPLAINABILITY=false
ENABLE_A11Y_TESTING=false
ENABLE_CUSTOM_METRICS=true
ENABLE_DATA_DRIFT_DETECTION=false

# Performance Configuration
MAX_CONCURRENT_PREDICTIONS=100
MAX_CONCURRENT_TRAINING=5
PREDICTION_TIMEOUT_MS=30000
TRAINING_TIMEOUT_MS=3600000
MAX_MEMORY_USAGE_PERCENT=80
MAX_CPU_USAGE_PERCENT=80
GC_INTERVAL_MS=300000
MODEL_CLEANUP_INTERVAL_MS=3600000

# External Integrations
TENSORFLOW_SERVING_URL=http://localhost:8501
MLFLOW_TRACKING_URI=http://localhost:5000
WEIGHTS_AND_BIASES_API_KEY=your-wandb-api-key
HUGGING_FACE_API_KEY=your-hf-api-key

# Data Sources
N8N_API_URL=http://localhost:5678/api
WORKFLOW_MONITOR_URL=http://localhost:3007
HEALTH_MONITOR_URL=http://localhost:3009
ANALYTICS_API_URL=http://localhost:3008

# Cloud Provider Configuration (for advanced features)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
AWS_S3_BUCKET=your-ml-models-bucket

AZURE_SUBSCRIPTION_ID=your-azure-subscription-id
AZURE_CLIENT_ID=your-azure-client-id
AZURE_CLIENT_SECRET=your-azure-client-secret
AZURE_TENANT_ID=your-azure-tenant-id

GCP_PROJECT_ID=your-gcp-project-id
GCP_SERVICE_ACCOUNT_KEY_PATH=./gcp-service-account.json

# Model Storage Configuration
MODEL_STORAGE_TYPE=local
MODEL_STORAGE_PATH=./models
S3_MODEL_BUCKET=your-model-bucket
AZURE_MODEL_CONTAINER=models
GCS_MODEL_BUCKET=your-gcs-bucket

# Notification Configuration
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR/DISCORD/WEBHOOK
EMAIL_SMTP_HOST=smtp.gmail.com
EMAIL_SMTP_PORT=587
EMAIL_USERNAME=your-email@gmail.com
EMAIL_PASSWORD=your-app-password

# Advanced Analytics
ENABLE_FEATURE_IMPORTANCE=true
ENABLE_SHAP_EXPLANATIONS=false
ENABLE_LIME_EXPLANATIONS=false
ENABLE_ADVERSARIAL_TESTING=false
ENABLE_FAIRNESS_TESTING=false

# Development & Testing
ENABLE_MOCK_DATA=false
ENABLE_TEST_ENDPOINTS=false
MOCK_FAILURE_RATE=0.1
ENABLE_DEBUGGING=false
DEBUG_MODEL_OUTPUTS=false

# Backup & Recovery
ENABLE_AUTOMATIC_BACKUPS=true
BACKUP_SCHEDULE=0 2 * * *
BACKUP_RETENTION_DAYS=30
BACKUP_STORAGE_PATH=./backups

# Compliance & Privacy
ENABLE_DATA_ANONYMIZATION=false
ENABLE_GDPR_COMPLIANCE=false
DATA_RETENTION_DAYS=365
ENABLE_AUDIT_LOGGING=true