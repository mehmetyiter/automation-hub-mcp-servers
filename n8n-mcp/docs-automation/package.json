{
  "name": "@n8n-mcp/docs-automation",
  "version": "1.0.0",
  "description": "Documentation automation tools for n8n-MCP",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "generate:api": "ts-node src/generators/api-docs.ts",
    "generate:changelog": "ts-node src/generators/changelog.ts",
    "generate:sdk-docs": "ts-node src/generators/sdk-docs.ts",
    "generate:all": "npm run generate:api && npm run generate:changelog && npm run generate:sdk-docs",
    "validate": "ts-node src/validators/validate-docs.ts",
    "serve": "ts-node src/server.ts",
    "test": "jest"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@microsoft/api-extractor": "^7.38.0",
    "@microsoft/api-documenter": "^7.23.0",
    "conventional-changelog": "^5.1.0",
    "conventional-changelog-cli": "^4.1.0",
    "typedoc": "^0.25.4",
    "swagger-jsdoc": "^6.2.8",
    "markdown-pdf": "^11.0.0",
    "marked": "^11.0.0",
    "highlight.js": "^11.9.0",
    "mermaid": "^10.6.1",
    "docsify": "^4.13.1",
    "vitepress": "^1.0.0-rc.31",
    "docusaurus": "^3.0.1",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "glob": "^10.3.10",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.6.0",
    "express": "^4.18.2",
    "chokidar": "^3.5.3",
    "ws": "^8.15.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.10",
    "@types/marked": "^5.0.2",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.14.202",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1"
  }
}